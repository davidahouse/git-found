{
  "name": "git-found",
  "main": "bin/git-found.js",
  "version": "1.0.0",
  "description": "A command line tool for finding & updating local branches from the git origin",
  "license": "MIT",
  "repository": "davidahouse/git-found",
  "preferGlobal": true,
  "type": "module",
  "bin": {
    "git-found": "bin/git-found.js"
  },
  "dependencies": {
    "better-queue": "^3.8.12",
    "chalk": "^5.1.2",
    "clear": "^0.1.0",
    "figlet": "^1.5.2",
    "project-version": "^2.0.0",
    "rc-house": "^1.4.0",
    "simple-git": "3.14.1"
  },
  "devDependencies": {
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0"
  },
  "scripts": {
    "lint": "eslint $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")",
    "lint-fix": "eslint --fix $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")"
  }
}
